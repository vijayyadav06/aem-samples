<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Product Detail Page (L3 Template) for the Medtronic.com site."
    jcr:primaryType="cq:Template"
    jcr:title="Medtronic.com Product Detail Page"
    allowedPaths="[/content/medtronic-com(/[^/]+)+]"
    ranking="{Long}1">
    <jcr:content
        cq:designPath="/etc/designs/medtronic-com"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="medtronic-com/components/pages/product-detail-page"
        pageLevel="L3">
        <content-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/iparsys">
            <product-detail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/product-detail"/>
        </content-par>
        <content-par2
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/iparsys">
            <similar-content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/similar-content"/>
            <title-text-list
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/title-text-list"/>
            <title-text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/title-text"/>
        </content-par2>
    </jcr:content>
</jcr:root>
