<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="{Long}550">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Secondary Navigation Item">
                <items jcr:primaryType="cq:WidgetCollection">
                    <back
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Back Link Text"
                        name="./backLink"
                        xtype="textfield"/>
                    <navigationText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link Text"
                        name="./navigationText"
                        xtype="textfield"/>
                    <navigationUrl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link URL"
                        name="./navigationUrl"
                        validator="function() {return Medtronic.Authoring.validateEManualUrl(this);}"
                        xtype="pathfield"/>
                    <submenu
                        jcr:primaryType="nt:unstructured"
                        defaultValue="{Boolean}true"
                        fieldLabel="Include Tertiary Navigation"
                        name="./submenu"
                        type="checkbox"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(box,value,isChecked){Medtronic.Authoring.NavigationMenu.showNavUrl.selectionChanged(box, isChecked);}"/>
                    </submenu>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
