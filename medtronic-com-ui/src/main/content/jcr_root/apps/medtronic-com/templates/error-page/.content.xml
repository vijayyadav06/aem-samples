<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="404 Error Page for the Medtronic.com site."
    jcr:primaryType="cq:Template"
    jcr:title="Medtronic.com 404 Error Page"
    allowedPaths="[/content/medtronic-com(/[^/]+)+(/[^/]+)]"
    ranking="{Long}1">
    <jcr:content
        cq:designPath="/etc/designs/medtronic-com"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="medtronic-com/components/pages/error-page">
        <content-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys"/>
        <content-par1 jcr:primaryType="nt:unstructured">
            <rich_text_editor
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/rich-text-editor"
                richText="&lt;p>Enter&amp;nbsp;{requestURL}&amp;nbsp;to display the requested pages URL within the Rich Text Editor dialog.&lt;/p>&#xa;"/>
        </content-par1>
    </jcr:content>
</jcr:root>
