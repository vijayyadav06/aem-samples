<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Condition Detail Page (L3 Template) for the Medtronic.com site."
    jcr:primaryType="cq:Template"
    jcr:title="Medtronic.com Condition Detail Page"
    allowedPaths="[/content/medtronic-com(/[^/]+)+]"
    ranking="{Long}1">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="medtronic-com/components/pages/condition-detail-page"
        pageLevel="L3">
        <condition-content-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/iparsys">
            <rich-text-editor
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/rich-text-editor"/>
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/accordion"/>
            <similar-content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/similar-content"/>
        </condition-content-par>
        <condition-hero
            jcr:primaryType="nt:unstructured"
            sling:resourceType="medtronic-com/components/system/single-item-parsys">
            <hero
                jcr:primaryType="nt:unstructured"
                sling:resourceType="medtronic-com/components/content/hero"/>
        </condition-hero>
    </jcr:content>
</jcr:root>
