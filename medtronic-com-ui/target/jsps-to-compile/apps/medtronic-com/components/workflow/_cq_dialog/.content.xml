<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="absoluteTime"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#Title">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <jobname
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Job Name"
                            name="jobname"/>
                    <jobdescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                            fieldLabel="Job Description"
                            name="jobdescription"/>
                    <sourceLanguage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/select"
                            fieldLabel="Source Language"
                            name="sourceLanguage">
                        <datasource
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="medtronic-com/components/workflow/languages"/>
                    </sourceLanguage>
                    <contentType
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/select"
                            fieldLabel="Content Type"
                            name="contentType"
                            typeHint="String">
                        <datasource
                                jcr:primaryType="nt:unstructured"
                                url="/content/ctctranslation/ctcconfig/getcontenttyps.html"
                                sling:resourceType="medtronic-com/components/workflow/datasource"/>
                    </contentType>
                    <email
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Notification Emails"
                            name="notifyemails"/>
                    <lsp
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/select"
                            fieldLabel="LSP"
                            name="lsp"
                            typeHint="String">
                        <datasource
                                jcr:primaryType="nt:unstructured"
                                url="/content/ctctranslation/ctcconfig/getlsps.html"
                                sling:resourceType="medtronic-com/components/workflow/datasource"/>
                    </lsp>
                    <sendAsJob
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="sendAsJob"
                            value="true"/>
                    <source
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="source"/>
                    <intarget
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="intarget"
                            value="true"/>
                    <onlylocked
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="onlylocked"
                            value="false"/>
                    <unlockdone
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="unlockdone"
                            value="false"/>
                    <donotsync
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="donotsync"
                            value="true"/>
                    <localtm
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="localtm"
                            value="false"/>
                    <targetLanguage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="getTargets"/>
                    <page
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="page"/>
                    <jobid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="jobid"/>
                    <formId
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                            name="formId"
                            value="translateProperties"/>
                    <workItemPath
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="medtronic-com/components/common/work-item-path"
                            name="workItemPath"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
